{
  "name": "simplequad-playground",
  "version": "1.0.0",
  "description": "examples and other shiz using simplequad and maybe not...",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "npm run clean && webpack",
    "clean": "rimraf *.worker.js app.bundle.js"
  },
  "author": "Richie Casto (https://github.com/rcasto)",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "simplequad": "^1.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.2",
    "babel-loader": "^8.0.6",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "worker-loader": "^2.0.0"
  }
}
